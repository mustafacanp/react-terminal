{
  "name": "react-terminal",
  "version": "2.0.0",
  "description": "A web-based terminal simulator built with React that provides an interactive Linux/Unix-like command-line interface in the browser",
  "keywords": [
    "react",
    "terminal",
    "simulator",
    "cli",
    "command-line",
    "linux",
    "unix",
    "bash",
    "web-terminal",
    "interactive"
  ],
  "bugs": {
    "url": "https://github.com/mustafacanp/react-terminal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mustafacanp/react-terminal.git"
  },
  "author": {
    "name": "Mustafa Can Palaz",
    "url": "https://mustafacanpalaz.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
